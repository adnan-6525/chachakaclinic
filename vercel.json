{
    "version": 2,
    "builds": [
      {
        "src": "protext/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      },
      {
        "src": "build_file.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }               
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "protext/wsgi.py"
      }
    ]
  }
  